@model IEnumerable<Document>
@if (Model.Count() > 0)
{
    <hr />
    <h4>Tillhörande document</h4>
    <p class="smaller">Klicka för att ladda ner</p>
    <ul class="none-listed">
        @foreach (Document Doc in Model)
        {
            <li>@Html.ActionLink(Doc.Name, "Download", new { id = Doc.Id })</li>
        }
    </ul>
}