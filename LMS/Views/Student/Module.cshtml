@model Module
@{
    ViewBag.Title = "Module";
}

@if (ViewBag.Documents != null)
{
    @Html.Partial("AllDocs", (IEnumerable<Document>)ViewBag.Documents)
}

<h2>Modul</h2>

<h4>@Model.Name</h4>
<p>@Model.Description</p>

<p>mellan <time datetime="@Model.StartDate.ToString("yyy-mm-dd")">@Model.StartDate.ToString(" yyyy-mm-dd ")</time> - och <time datetime="@Model.EndDate.ToString("yyyy-mm-dd")">@Model.EndDate.ToString(" yyyy-mm-dd ")</time></p>
<br />

@Html.Partial("ActivitiesTable", Model.Activities)